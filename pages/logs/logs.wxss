.middle-box{
  margin-top: 20px;
  flex-direction: column;
  align-items: center;
  background-color: rgb(45, 210, 30);
}
.header {
  width: 350rpx;
  height: 350rpx;
  border-radius: 50%;
  overflow:hidden;
}
.userinfo-nickname {
  color: #aaa;
  align-items: center;
}
.image{
  overflow: hidden;
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
  align-items: center;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  align-items: center;
}